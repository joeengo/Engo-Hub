local function XRQCTM_fake_script() -- Main.Dragify 
	local script = Instance.new('LocalScript', Main)

	script.Parent.Draggable = true
	script.Parent.Active = true
	
	local UserInputService = game:GetService("UserInputService")
	
	local function onInputBegan(input, gameProcessed)
		if input.KeyCode == Enum.KeyCode.LeftAlt then
			script.Parent.Parent.Enabled = not script.Parent.Parent.Enabled
		end
	end
	
	UserInputService.InputBegan:Connect(onInputBegan)
end
coroutine.wrap(XRQCTM_fake_script)()
local function TRDJP_fake_script() -- intro.Script 
	local script = Instance.new('Script', intro)

	script.Parent.Parent.Enabled = false

	script.Parent.Enabled = true
	local text = script.Parent.Frame.TextLabel
	local text2 = script.Parent.Frame.TextLabel1
	local text3 = script.Parent.Frame.TextLabel2
	local box = script.Parent.Frame
	box.Size = UDim2.new(0, 172, 0, 0)
	text.TextTransparency = 1
	text2.TextTransparency = 1
	text3.TextTransparency = 1
	box:TweenSize(UDim2.new(0, 172, 0, 172), "Out", "Quint", 1.5)
	wait(1.5)
	for i = 1, 30 do
		text.TextTransparency = text.TextTransparency - 0.05
		wait(0.001)
	end
	
	for i = 1, 30 do
		text2.TextTransparency = text2.TextTransparency - 0.05
		wait(0.001)
	end
	
	for i = 1, 30 do
		text3.TextTransparency = text3.TextTransparency - 0.05
		wait(0.001)
	end
	wait(1)
	box:TweenSize(UDim2.new(0, 172, 0, 0), "In", "Quint", 0.5)
	wait(0.5)
	script.Parent.Parent.Enabled = true
	script.Parent:Destroy()
	
end
coroutine.wrap(TRDJP_fake_script)()
