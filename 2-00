local function LJXYCPC_fake_script() -- SideBar_2.TabManager 
	local script = Instance.new('LocalScript', SideBar_2)

	--made by Joe Engo.
	local sel = script.Parent.Parent.Selected
	local gtab = script.Parent.Parent.Parent.GamesTab
	local ctab = gtab.Parent.CreditsTab
	local stab = gtab.Parent.ScriptsTab
	local sbtn = script.Parent.Scripts
	local gbtn = sbtn.Parent.Games
	local cbtn = sbtn.Parent.Credits
	local home = gtab.Parent.HomePage
	local hbtn = script.Parent.Home
	local TS = game:GetService("TweenService")
	local popo = UDim2.new(0, 0, 0.508, 0)
	local posC = UDim2.new(0, 0, 0.382, 0)
	local posH = UDim2.new(0, 0, 0.001, 0)
	local posS = UDim2.new(0, 0, 0.253, 0)
	local posG = UDim2.new(0, 0, 0.127, 0)
	
	local wait2 = 0.25
	
	sel.Position = posH
	
	gtab.Visible = false
	home.Visible = true
	ctab.Visible = false
	stab.Visible = false
	
	
	function move(pos)
		sel:TweenPosition(pos, "Out", "Quint", 0.5)
	end
	
	sbtn.MouseButton1Click:Connect(function()
		move(posS)
		wait(wait2)
		gtab.Visible = false
		home.Visible = false
		ctab.Visible = false
		stab.Visible = true
	
	end)
	
	gbtn.MouseButton1Click:Connect(function()
		move(posG)
		wait(wait2)
		ctab.Visible = false
		home.Visible = false
		stab.Visible = false
		gtab.Visible = true
	
	end)
	
	cbtn.MouseButton1Click:Connect(function()
		move(posC)
		wait(wait2)
		stab.Visible = false
		home.Visible = false
		gtab.Visible = false
		ctab.Visible = true
	
	end)
	
	hbtn.MouseButton1Click:Connect(function()
		move(posH)
		wait(wait2)
		stab.Visible = false
		home.Visible = true
		gtab.Visible = false
		ctab.Visible = false
	
	end)
end
coroutine.wrap(LJXYCPC_fake_script)()
